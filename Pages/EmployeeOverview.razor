@page "/EmployeeOverview"
@inherits EmployeeOverviewBase

<h3>EmployeeOverview</h3>

<table>
    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
    </tr>
    <tbody>
        @foreach (var item in Empoloyees)
        {

            <tr>
                <td>
                @item.Id 
                </td>
                <td>
                    @item.Name
                </td>

                
            </tr>

        }

    </tbody>
</table>
